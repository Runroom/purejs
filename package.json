{
  "name": "@runroom/purejs",
  "version": "3.1.1",
  "description": "PureJS is a pack of vanilla javascript functions exported as Nodejs modules.",
  "author": "Italo Devoto Ramella <italodr@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:Runroom/purejs.git"
  },
  "license": "MIT",
  "keywords": [
    "runroom",
    "starter-kit",
    "library",
    "no-jquery"
  ],
  "main": "dist/purejs.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean:node": "rm -rf dist/*",
    "clean:browser": "rm -rf validator*.js",
    "clean": "run-p clean:*",
    "build:node": "tsc",
    "dev": "webpack --mode=development",
    "build:webpack": "webpack --mode=production",
    "build": "run-p build:*",
    "pretest": "run-p build:*",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.18",
    "@babel/core": "^7.19",
    "@babel/eslint-parser": "^7.19",
    "@babel/plugin-transform-object-assign": "^7.18",
    "@babel/plugin-transform-runtime": "^7.19",
    "@babel/preset-env": "^7.19",
    "@babel/register": "^7.18",
    "@babel/runtime": "^7.19",
    "@types/jest": "^29.0",
    "@types/node": "^18.7",
    "babel-loader": "^8.2",
    "eslint": "^8.23",
    "eslint-config-airbnb-base": "^15.0",
    "eslint-config-prettier": "^8.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-prettier": "^4.2",
    "eslint-webpack-plugin": "^3.2",
    "jest": "^29.0",
    "jest-environment-jsdom": "^29.0",
    "npm-run-all": "^4.1",
    "terser-webpack-plugin": "^5.3",
    "ts-jest": "^29.0",
    "ts-loader": "^9.4",
    "ts-node": "^10.9",
    "typescript": "^4.8",
    "webpack": "^5.74",
    "webpack-bundle-analyzer": "^4.6",
    "webpack-cli": "^4.10"
  }
}
