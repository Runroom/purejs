{
  "name": "@runroom/purejs",
  "version": "3.0.0",
  "description": "PureJS is a pack of vanilla javascript functions exported as Nodejs modules.",
  "author": "Italo Devoto Ramella <italodr@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:Runroom/purejs.git"
  },
  "license": "MIT",
  "keywords": [
    "runroom",
    "starter-kit",
    "library",
    "no-jquery"
  ],
  "main": "purejs.min.js",
  "typings": "index.d.ts",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "clean:node": "rm -rf index.min.js lib/*",
    "clean:browser": "rm -rf validator*.js",
    "clean": "yarn clean:node && yarn clean:browser",
    "minify": "uglifyjs validator.js -o validator.min.js  --compress --mangle --comments /Copyright/",
    "build:node": "tsc",
    "dev": "webpack --mode=development",
    "build": "webpack -p --mode=production",
    "pretest": "yarn build",
    "test": "karma start"
  },
  "dependencies": {
    "@types/node": "13.7.0",
    "puppeteer": "2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/register": "7.8.3",
    "@babel/runtime": "^7.8.4",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-transform-object-assign": "6.22.0",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "istanbul-instrumenter-loader": "3.0.1",
    "karma": "4.4.1",
    "karma-babel-preprocessor": "8.0.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.1",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-coveralls": "2.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon-chai": "2.0.2",
    "karma-webpack": "^4.0.2",
    "mocha": "7.0.1",
    "sinon": "8.1.1",
    "sinon-chai": "3.4.0",
    "terser-webpack-plugin": "2.3.4",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "util": "^0.12.1",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.10",
    "yargs": "15.1.0"
  },
  "peerDependencies": {
    "prettier": "1.19.1"
  }
}
